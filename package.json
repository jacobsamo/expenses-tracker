{
  "name": "expense-tracker",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "bun@1.2.8",
  "scripts": {
    "dev": "astro dev --port=3000",
    "build": "astro check && astro build",
    "preview": "bun run build && wrangler pages dev",
    "astro": "astro",
    "deploy": "bun run build && wrangler pages deploy",
    "cf-typegen": "wrangler types",
    "format": "prettier --write \"**/*.{ts,tsx,md,astro,mdx}\"",
    "clean": "rm -rf .astro && rm -rf node_modules && rm -rf dist && rm -rf .wrangler",
    "lint": "eslint .",
    "ui": "bunx --bun shadcn@canary",
    "db:auth": "bunx @better-auth/cli generate --config ./src/lib/auth/index.ts --output ./src/lib/db/schemas",
    "db:studio": "drizzle-kit studio",
    "db:studio:prd": "cross-env NODE_ENV=production drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:setup": "touch expenses-local.db && bun db:generate",
    "db:local-start": "turso dev --db-file expense-local.db",
    "kv-create": "bunx wrangler kv namespace create 'SESSION'"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.12",
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.0",
    "@astrojs/react": "^4.2.4",
    "@aws-sdk/client-s3": "^3.787.0",
    "@better-fetch/fetch": "^1.1.18",
    "@hookform/resolvers": "^5.0.1",
    "@libsql/client": "^0.15.4",
    "@radix-ui/react-accordion": "^1.2.7",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-aspect-ratio": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.6",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-collapsible": "^1.1.7",
    "@radix-ui/react-context-menu": "^2.2.11",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-hover-card": "^1.1.10",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-menubar": "^1.1.11",
    "@radix-ui/react-navigation-menu": "^1.2.9",
    "@radix-ui/react-popover": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.3",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.8",
    "@radix-ui/react-toast": "^1.2.10",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-toggle-group": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "ai": "^4.3.9",
    "astro": "^5.7.4",
    "autoprefixer": "^10.4.21",
    "better-auth": "^1.2.7",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.42.0",
    "drizzle-zod": "^0.7.1",
    "embla-carousel-react": "^8.6.0",
    "heic2any": "^0.0.4",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.503.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.56.0",
    "react-resizable-panels": "^2.1.8",
    "rechart": "^0.0.1",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "vite": "^6.3.2",
    "wrangler": "^4.12.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
